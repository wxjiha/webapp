<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User List</title>
</head>
<body>
    <h1>List of Users</h1>
    <table>
        <thead>
            <tr>
                <th>Username</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% if (users.length > 0) { %>
                <% users.forEach(user => { %>
                    <tr>
                        <td><%= user.userName %></td>
                        <td><%= user.email %></td>
                    </tr>
                <% }); %>
            <% } else { %>
                <tr>
                    <td colspan="4">No users found.</td>
                </tr>
            <% } %>
        </tbody>
    </table>
</body>
</html>
